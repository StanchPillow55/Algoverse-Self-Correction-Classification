{
  "name": "Simple OpenAI Ensemble",
  "description": "2-model ensemble using OpenAI models",
  "provider": "openai",
  "ensemble_size": 2,
  "ensemble_models": [
    "gpt-4o-mini",
    "gpt-4o"
  ],
  "voting_strategy": "majority_with_confidence",
  "features": {
    "enable_confidence": true,
    "enable_error_awareness": false,
    "enable_multi_turn": true
  },
  "cost_optimization": {
    "enabled": false,
    "max_cost_per_question": 1.0,
    "early_stopping": false
  },
  "experiment_settings": {
    "max_turns": 3,
    "temperature": 0.2,
    "max_tokens": 1024
  }
}